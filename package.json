{
  "version": "3.0.0",
  "name": "piraces.dev",
  "description": "Project for the site piraces.dev, written in GatsbyJS based in the Lumen theme",
  "main": "n/a",
  "scripts": {
    "develop": "npm run clean && gatsby develop",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss && npm run flow",
    "test": "jest --config ./tests/jest-config.js",
    "test:coverage": "jest --coverage --config ./tests/jest-config.js",
    "test:watch": "jest --watch --config ./tests/jest-config.js"
  },
  "repository": "https://github.com/piraces/piraces.dev",
  "author": "Raúl Piracés <raul.piraces@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "netlify-cms",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme"
  ],
