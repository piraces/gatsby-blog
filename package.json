{
  "version": "3.0.0",
  "name": "piraces.dev",
  "description": "Project for the site piraces.dev, written in GatsbyJS based in the Lumen theme",
  "main": "n/a",
  "scripts": {
    "develop": "npm run clean && gatsby develop",
    "build": "gatsby build --prefix-paths",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "install-peers": "install-peers",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "lint:js:fix": "eslint --cache --ext .js,.jsx --ignore-pattern public --fix .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix --fix && npm run flow",
    "lint": "npm run lint:js && npm run lint:scss && npm run flow",
    "updateSnapshots": "jest --config ./tests/jest-config.js --updateSnapshot",
    "test": "jest --config ./tests/jest-config.js",
    "test:coverage": "jest --coverage --config ./tests/jest-config.js",
    "test:watch": "jest --watch --config ./tests/jest-config.js"
  },
  "repository": "https://github.com/piraces/piraces.dev",
  "author": "Raúl Piracés <raul.piraces@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.52.0",
    "gatsby": "^2.19.23",
    "gatsby-cli": "^2.9.0",
    "gatsby-link": "^2.2.29",
    "gatsby-plugin-brotli": "^1.3.1",
    "gatsby-plugin-catch-links": "^2.1.26",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-flow": "^1.1.19",
    "gatsby-plugin-google-gtag": "^2.0.0",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-katex": "^3.1.24",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-remark-responsive-iframe": "^2.2.32",
    "gatsby-remark-smartypants": "^2.1.21",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.3.16",
    "invariant": "^2.2.4",
    "katex": "^0.11.1",
    "lodash": "^4.17.15",
    "moment": "2.27.0",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.8.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.4",
    "autoprefixer": "9.8.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "browserslist": "4.12.2",
    "concurrently": "5.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.2",
    "eslint-watch": "6.0.1",
    "flow-bin": "^0.128.0",
    "identity-obj-proxy": "3.0.0",
    "immutable": "^3.8.2",
    "install-peers-cli": "^2.2.0",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "lost": "8.3.1",
    "postcss-pxtorem": "5.1.1",
    "react-test-renderer": "16.13.1",
    "redux": "^4.0.5",
    "rimraf": "3.0.2",
    "slate": "^0.58.0",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6"
  }
}
